.Patients
  height: 100%;
  overflow-y: auto;
  max-width: 600px;
  margin: 0 auto;

  .v-list  
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    
  .v-list-tile
    padding-left: 0;
    padding-right: 0;

  .actions
    for i in (0..5)
      .floating-btn:nth-of-type({i})
        .v-btn
          left: i * 100px !important;